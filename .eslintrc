{
    "env": {
        "browser": true,
        "node": true
    },
    "parseOptions": {
        "ecmaVerions": 6
    },
    "rules": {
        "semi": "error",
        "quotes": [
            0,
            "single",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ]
    }
}